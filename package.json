{
  "name": "kazticket-commercial",
  "description": "kazticket-commercial",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "engines": {
    "node": ">=18.18.2"
  },
  "scripts": {
    "dev": "next",
    "build:dev": "cp .env.development .env.local && next build",
    "build:prod": "cp .env.production .env.local && next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@upstash/redis": "1.31.6",
    "antd": "5.21.6",
    "cookies-next": "4.1.0",
    "js-cookie": "3.0.5",
    "jwt-decode": "4.0.0",
    "moment": "2.30.1",
    "next": "^14.0.3",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-phone-input-2": "2.15.1",
    "react-quill": "2.0.0",
    "react-swipeable": "7.0.1",
    "react-virtuoso": "4.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.1",
    "@commitlint/config-conventional": "18.4.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@meinefinsternis/react-horizontal-date-picker": "1.0.1",
    "@next/bundle-analyzer": "14.1.4",
    "@types/group-array": "1.0.3",
    "@types/node": "20.9.0",
    "@types/qrcode": "1.5.5",
    "@types/react": "18.2.37",
    "@types/react-beautiful-dnd": "13.1.8",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "autoprefixer": "10.4.16",
    "cross-env": "7.0.3",
    "cssnano": "6.1.2",
    "date-fns": "3.6.0",
    "dayjs": "1.11.10",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "framer-motion": "11.0.3",
    "group-array": "1.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-sort-json": "3.1.0",
    "qrcode": "1.5.3",
    "react-facebook-pixel": "1.0.4",
    "react-phone-number-input": "3.3.9",
    "react-snowfall": "1.2.1",
    "schema-dts": "1.1.2",
